<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav_graph"
    app:startDestination="@id/navigation_new_books">

    <fragment
        android:id="@+id/navigation_new_books"
        android:name="app.library.features.home.newbooks.NewBooksFragment"
        android:label="@string/history_title"
        tools:layout="@layout/fragment_new_books" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="app.library.features.home.search.SearchFragment"
        android:label="@string/search_title"
        tools:layout="@layout/fragment_search" />

    <fragment
        android:id="@+id/navigation_bookmark"
        android:name="app.library.features.home.bookmark.BookmarkFragment"
        android:label="@string/bookmark_title"
        tools:layout="@layout/fragment_bookmark" />

    <fragment
        android:id="@+id/navigation_history"
        android:name="app.library.features.home.history.HistoryFragment"
        android:label="@string/history_title"
        tools:layout="@layout/fragment_history" />

</navigation>
